{
  "manifest_version": 2,

  "name": "Frontpage",
    "description": "This extension saves the page to your frontpage",
  "version": "1.0",

  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
      "activeTab",
      "pageCapture",
      "https://ajax.googleapis.com/",
      "cookies",
      "http://localhost:3000/"
  ]
}
